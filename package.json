{
  "name": "cl-nestjs-opentelemetry",
  "version": "0.1.1",
  "description": "一个全面的NestJS OpenTelemetry集成库，支持日志记录、指标和追踪功能",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaslui/cl-nestjs-opentelemetry.git"
  },
  "homepage": "https://github.com/chaslui/cl-nestjs-opentelemetry",
  "license": "MIT",
  "scripts": {
    "playground": "tsx playground/index.ts",
    "playground:build": "rimraf dist-playground && unbuild --config playground.config.ts && chmod +x dist-playground/index.mjs",
    "playground:run": "node dist-playground/index.mjs",
    "dev": "tsx playground/index.ts",
    "build": "rimraf dist && unbuild",
    "build:dev": "unbuild --stub",
    "build:all": "pnpm build && pnpm playground:build",
    "build:safe": "NODE_OPTIONS=\"--max-old-space-size=8192\" pnpm build",
    "build:watch": "unbuild --watch",
    "build:analyze": "pnpm build && ls -la dist/ && echo 'Source maps generated:'",
    "clean": "rimraf node_modules pnpm-lock.yaml dist && pnpm install",
    "clean:build": "./scripts/clean-install.sh",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:modules": "node test-modules.js",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*": "prettier -w -u"
  },
  "keywords": [
    "nestjs",
    "opentelemetry",
    "observability",
    "logging",
    "metrics",
    "tracing",
    "winston",
    "monitoring",
    "typescript",
    "nodejs"
  ],
  "author": "ChasLui <chaslui@outlook.com> (https://twitter.com/chaslui1)",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.15.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.2.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-jaeger": "^1.30.0",
    "@opentelemetry/exporter-prometheus": "^0.56.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.20",
    "@nestjs/testing": "^10.4.20",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild-decorators": "^1.0.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "tsx": "^4.20.5",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "bugs": {
    "url": "https://github.com/chaslui/cl-nestjs-opentelemetry/issues"
  },
  "directories": {
    "test": "tests"
  },
  "volta": {
    "node": "22.18.0",
    "pnpm": "10.15.0"
  }
}
